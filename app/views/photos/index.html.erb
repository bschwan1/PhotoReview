<h1>Listing of Photos to Review</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Filename</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.title %></td>
    <td><%= photo.description %></td>
    <td><%= photo.filename%></td>
    <td><%= link_to 'Show', photo %></td>
    <td>&nbsp</td>
    <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Add a New Photo', new_photo_path %>
